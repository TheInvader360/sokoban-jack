class Player {
    field int x, y;

    constructor Player new(int ax, int ay) {
        let x = ax;
        let y = ay;

        return this;
    }

    method int getX() {
        return x;
    }

    method int getY() {
        return y;
    }

    method void moveTo(int aX, int aY) {
        let x = aX;
        let y = aY;

        return;
    }

    method void dispose() {
        do Memory.deAlloc(this);

        return;
    }
}
