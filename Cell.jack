class Cell {
    field CellType typeOf;
    field bool hasBox;

    constructor Cell new(CellType aTypeOf, bool aHasBox) {
        let typeOf = aTypeOf;
        let hasBox = aHasBox;

        return this;
    }

    method CellType getTypeOf() {
        return typeOf;
    }

    method bool hasBox() {
        return hasBox;
    }

    method void dispose() {
        do Memory.deAlloc(this);
        return;
    }
}
